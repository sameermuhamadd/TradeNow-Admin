                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-12">


                                <div class="card">
                                    <div class="card-header">
                                        <strong class="card-title">Complain Detail</strong>
                                    </div>

                                    <div class="card-body">
                                        <div class="typo-headers">
                                            <h3 id="subject_complain"></h3>

                                        </div>
                                        <div class="typo-articles">
                                            <p id="complain_description">
                                                @Html.DisplayFor(modelitem => Model.Complain_Description)
                                            </p>
                                           
                                        </div>

                                        <div class="vue-misc">
                                        
                                            <div class="row">
                                                <div class="col-md-6">

                                                    <address class="mt-3">
                                                        <strong>Date</strong>
                                                        <br> @Html.DisplayFor(modelitem => Model.Date)
                                                        <strong>Status</strong>
                                                        <br> @Html.DisplayFor(modelitem => Model.Complain_Satus)
                                                        <strong>Type</strong>
                                                        <br> @Html.DisplayFor(modelitem => Model.Complain_Type)
                                                        <strong>ID</strong>
                                                        <br> @Html.DisplayFor(modelitem => Model.Complain_Id)

                                                    </address>
                                                </div>

                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>


                        </div>
                    </div>
                </div>
