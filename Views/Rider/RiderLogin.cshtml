                            <div class="user-details">

                                <div class="input-box">
                                    <span class="details">Email</span>
                                    <input id="F_name" type="email" placeholder="Enter Your Email" required>
                                </div>

                                <label id="invalid_msg" , style="color: red;visibility: hidden;"></label>
                                <br>
                                <div class="input-box">
                                    <span class="details">Password</span>
                                    <input id="L_name" type="password" placeholder="Enter your Password" required>
                                    <label id="invalid_email" , style="color: red;visibility: hidden;"></label>
                                </div>
                                <br>

                                <br>
                                <div class="input-box">
                                    <span class="details">Confirm Password</span>
                                    <input id="email_ID" type="password" placeholder="Password" required>
                                </div>
                                <br>
                                <label id="invalid_msg" , style="color: red;visibility: hidden;"></label>
                                <br>
