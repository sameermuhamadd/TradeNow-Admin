            <!-- MAIN CONTENT-->
            <div class="main-content">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">




                        <div class="table-responsive table-responsive-data2">
                            <form action="complain_Detail.html" onsubmit="return newPage()">
                                <table class="table table-data2">
                                    <thead>
                                        <tr>

                                            <th>ID</th>
                                            <th>AD ID</th>
                                            <th>User ID</th>
                                            <th>Reason</th>
                                            <!-- <th>status</th> -->
                                            <th>Date</th>
                                        </tr>
                                    </thead>
                                    <tbody id="tableData"></tbody>

                                    @foreach (var item in Model)
                                    {
                                        <tr class="tr-shadow">
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Report_Id)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Ad_Id)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.User_Id)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Report_Reason)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Date_Reported)
                                            </td>
                                            <td>
                                                <a href=@Url.Action("ComplainDetail","Complain")>
                                                    <div class="table-data-feature">

                                                        <button class="item" data-toggle="tooltip" data-placement="top" title="View">
                                                            <i class="zmdi zmdi-mail-send"></i>
                                                        </button>
                                                    </div>
                                                </a>
                                            </td>
                                        </tr>
                                    }
                                </table>
                        </div>
                        </form>
