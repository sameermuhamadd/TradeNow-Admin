    <link href="~/Content/Template/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="~/Content/Template/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="~/Content/Template/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="~/Content/Template/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="~/Content/Template/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="~/Content/Template/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/Content/Template/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">
    <!-- Main CSS-->
    <link href="~/Content/Template/css/theme.css" rel="stylesheet" media="all">
    <!--For The Table Thingy Dynamic-->
</head>
<body class="animsition">
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        <header class="header-mobile d-block d-lg-none">
            <div class="header-mobile__bar">
                <div class="container-fluid">
                    <div class="header-mobile-inner">
                        <a class="logo" href="index.html">
                            <img src="images/icon/logo.png" alt="CoolAdmin" />
                        </a>
                        <button class="hamburger hamburger--slider" type="button">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <nav class="navbar-mobile">
                <div class="container-fluid">
                    <ul class="navbar-mobile__list list-unstyled">
                        <li class="has-sub">
                            <a class="js-arrow" href="#">
                                <i class="fas fa-tachometer-alt"></i>Dashboard
                            </a>
                            <ul class="navbar-mobile-sub__list list-unstyled js-sub-list">
                                <li>
                                    <a href="index4.html">Open Dashboard</a>
                                </li>
                            </ul>
                        </li>
                        <li>
                            <a href="form.html">
                                <i class="fas fa-chart-bar"></i>Rider Registration
                            </a>
                        </li>
                        <li class="active">
                            <a href="table.html">
                                <i class="fas fa-table"></i>Order History
                            </a>
                        </li>

                        <li class="has-sub">
                            <a class="js-arrow" href="#">
                                <i class="fas fa-copy"></i>Admin
                            </a>
                            <ul class="list-unstyled navbar__sub-list js-sub-list">
                                <li>
                                    <a href="login.html">Login</a>
                                </li>

                            </ul>
                        </li>

                    </ul>
                </div>
            </nav>
        </header>
        <!-- END HEADER MOBILE-->
        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <a href="#">
                    <span>Order History</span>
                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list">
                        <li class="has-sub">
                            <a class="js-arrow" href="#">
                                <i class="fas fa-tachometer-alt"></i>Dashboard
                            </a>
                            <ul class="list-unstyled navbar__sub-list js-sub-list">

                                <li>
                                    <a href="@Url.Action("Index","Home")">Open Dashboard</a>
                                </li>
                            </ul>
                        </li>
                        <li>
                            <a href="@Url.Action("RiderLogin","Rider")">
                                <i class="fas fa-chart-bar"></i>Rider Registration
                            </a>
                        </li>
                        <li class="active">
                            <a href="@Url.Action("ViewOrder","Order")">
                                <i class="fas fa-table"></i>Order History
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("ComplainList","Complain")">
                                <i class="fas fa-table"></i>Complains
                            </a>
                        </li>

                        <li>
                            <a href="@Url.Action("RiderList","Rider")">
                                <i class="fas fa-table"></i>Riders List
                            </a>
                        </li>
                        <li class="has-sub">
                            <a class="js-arrow" href="#">
                                <i class="fas fa-copy"></i>Admin
                            </a>
                            <ul class="list-unstyled navbar__sub-list js-sub-list">
                                <li>
                                    <a href="@Url.Action("Login","Account")">Login</a>
                                </li>

                            </ul>
                        </li>

                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap">
                            <form class="form-header" action="" method="POST">
                                @*<input class="au-input au-input--xl" type="text" name="search" placeholder="Search for datas &amp; reports..." />
                                <button class="au-btn--submit" type="submit">
                                    <i class="zmdi zmdi-search"></i>
                                </button>*@
                            </form>
                            <div class="header-button">

                                <div class="account-wrap">
                                    <div class="account-item clearfix js-item-menu">
                                       
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <!-- END HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
            <div class="main-content">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">




                        <div class="row m-t-30">
                            <div class="col-md-12">
                                <!-- DATA TABLE-->
                                <div class="table-responsive m-b-40">
                                    <table class="table table-borderless table-data3">
                                        <thead>
                                            <tr>
                                                <th>Order ID</th>
                                                <th>Buyer ID</th>
                                                <th>Seller ID</th>
                                                <th>AD ID</th>
                                                <th>Pickup Address</th>
                                                <th>Shipping Address</th>
                                                <th>Buyer Contact</th>
                                                <th>Seller Contact</th>
                                                <th>Type</th>
                                                <th>Amount</th>
                                                <th>Date</th>
                                                <th>Seller Confirm</th>
                                                <th>Buyer Confirm</th>
                                                <th>Information</th>
                                            </tr>
                                        </thead>
                                        <tbody id="tableData">
                                            @foreach (var item in Model)
                                            {
                                                <tr>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Order_Id)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Buyer_Id)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Seller_Id)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Ad_Id)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Pickup_Address)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Shipping_Address)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.ContactNum_Buyer)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.ContactNum_Seller)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Order_Type)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Order_Amount)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Order_Date)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.BuyerConfirmed)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.SellerConfirmed)
                                                    </td>
                                                    <td>
                                                        @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                                                        @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                                                        @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
                                                    </td>
                                                    </tr>
                                            }
                                           
                                          </tbody>
                                          
                                    </table>
                                </div>

                                <!-- END DATA TABLE-->
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="copyright">
                                    <p>Copyright © 2020 TradeNow. All rights reserved.</p>
                                </div>
                            </div>
                        </div>
                        @*<div class="row">
                            <div class="col-md-12">
                                <div class="copyright">
                                    <button onclick="getLength()">Click</button>
                                </div>
                            </div>
                        </div>*@
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script>
        function getLength()
        {
            alert(document.getElementById('tableData').cells.length)
        }
    </script>
    <!-- Jquery JS-->
    <script src="~/Content/Template/vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="~/Content/Template/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="~/Content/Template/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="~/Content/Template/vendor/slick/slick.min.js">
    </script>
    <script src="~/Content/Template/vendor/wow/wow.min.js"></script>
    <script src="~/Content/Template/vendor/animsition/animsition.min.js"></script>
    <script src="~/Content/Template/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js">
    </script>
    <script src="~/Content/Template/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="~/Content/Template/vendor/counter-up/jquery.counterup.min.js">
    </script>
    <script src="~/Content/Template/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="~/Content/Template/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/Content/Template/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="~/Content/Template/vendor/select2/select2.min.js">
    </script>
    <!-- Main JS-->
    <script src="~/Content/Template/js/main.js"></script>
   
</body>
</html>
<!-- end document-->
